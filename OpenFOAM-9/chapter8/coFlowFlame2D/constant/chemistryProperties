chemistryType
{
    chemistrySolver   ode;
    //chemistryThermo   psi;
}

chemistry           on;

initialChemicalTimeStep 1e-07;

sequentialCoeffs
{
    cTauChem        0.001;
}

EulerImplicitCoeffs
{
    cTauChem        0.05;
    equilibriumRateLimiter off;
}

odeCoeffs
{
     /*solver          SIBS;
     eps             1e-4;
     scale           1;*/
     solver          Trapezoid;
     absTol          1e-8;
     relTol          1e-1;
}
