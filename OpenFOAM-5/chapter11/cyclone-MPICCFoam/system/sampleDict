FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       raw;

sets
(
    line
    {
        type    uniform;
        axis    x;
        start   (-0.401 -0.399 -1);
        end     (0.45 0.399 1.1);
        nPoints 100;
    }
);

fields          (Ut p);

