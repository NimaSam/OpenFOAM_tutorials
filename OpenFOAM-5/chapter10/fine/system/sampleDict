FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;

surfaceFormat raw;

interpolationScheme cellPointFace;

fields
(
 magU
);

sets
(
    x_-025
    {
      type face;
      axis xyz;
      start (-0.25 0.02 -0.3);
      end (-0.25 0.02 0.3);	
    }

    x_-02
    {
      type face;
      axis xyz;
      start (-0.2 0.02 -0.3);
      end (-0.2 0.02 0.3);
    }

    x_-015
    {
      type face;
      axis xyz;
      start (-0.15 0.02 -0.3);
      end (-0.15 0.02 0.3);
    }

    x_015
    {
      type face;
      axis xyz;
      start (0.15 0.02 -0.3);
      end (0.15 0.02 0.3);	
    }

    x_02
    {
      type face;
      axis xyz;
      start (0.2 0.02 -0.3);
      end (0.2 0.02 0.3);
    }

    x_025
    {
      type face;
      axis xyz;
      start (0.25 0.02 -0.3);
      end (0.25 0.02 0.3);
    }
);

surfaces ();

