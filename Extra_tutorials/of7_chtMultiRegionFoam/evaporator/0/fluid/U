FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../properties"

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    FluidInlet
    {
        type            flowRateInletVelocity;
        massFlowRate    $mfr;
        rhoInlet        997.67;
        extrapolateProfile yes;
        value           uniform (0 0 0);
    }
    FluidOutlet
    {
        type            fluxCorrectedVelocity;//zeroGradient;//inletOutlet;
        inletValue	uniform (0 0 0);
        value		uniform (0 0 0);
    }
    fluid_to_solid // Name is built by splitMeshRegion
    {
        type            noSlip;
    } 
    MicrochannelTop
    {
    	type		noSlip;
    }
}

