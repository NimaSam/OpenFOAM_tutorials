FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "0";
    object      properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

mfr		constant 8.1120E-05; //mass flow rate kg/s
kappaGlassWool  0.04; //0.023-0.04
kappaRigid10K   0.621;
KappaPyrex  	1.2;
KappaOring  	0.24;

Tair    uniform 295.65; //room temperature or air temperature
Tin     uniform 295.65; // inlet temperature

hConv	uniform 25; //for air free convection 10-25

