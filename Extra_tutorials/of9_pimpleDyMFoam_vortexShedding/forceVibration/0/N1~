FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    topAndBottom
    {
        type            calculated;
    } 
    outlet
    {
        type            calculated;
    }
    inlet
    {
        type            calculated;
    }
    frontAndBack
    {
        type            empty;
    }
    cylinder
    {
        type            calculated;
    }
}

