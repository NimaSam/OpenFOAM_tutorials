FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 -1 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    inlet
    {
        type            timeVaryingMappedFixedValue;
	setAverage      off;
        offset          0;
    }
    outlet
    {
        type            zeroGradient;
    }   
    atm
    {
        type            slip;
    }
    
    "(buildings|ground)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}

// ************************************************************************* //
