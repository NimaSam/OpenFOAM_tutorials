FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 50;

boundaryField
{
    movingWall
    {
        type            fixedValue;
	value		uniform 50;
    }

    fixedWalls
    {
        type            fixedValue;
	value		uniform 75;
    }

    frontAndBack
    {
        type            empty;
    }
}

