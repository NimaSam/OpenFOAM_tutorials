FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    #include "include/slipPatches"

    inlet
    {
        type            timeVaryingMappedFixedValue;
	setAverage      off;
        offset          0;
    }

    outlet
    {
        type            zeroGradient;
    }

    ".*"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
}

